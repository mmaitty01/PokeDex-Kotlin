        android:supportsRtl="true"
        android:name=".MainApplication"
        android:theme="@style/Theme.PokeDex0528">
        <activity android:name=".userinterface.activity.SplashScreenActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".userinterface.activity.MainActivity"
            android:exported="true"></activity>
        <activity android:name=".userinterface.activity.DetailActivity"
            android:exported="true">

        </activity>
    </application>

</manifest>